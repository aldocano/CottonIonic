<div class="list">
	<div class="item item-checkbox checkbox-balanced item-checkbox-right popup_list" ng-repeat="std in stdArray">
      <label class="checkbox">
        <input type="checkbox" ng-model="std.checked"
        ng-checked="std.checked" ng-change="changeStd({{$index}})">   
      </label><h2 class="popup_label">{{std.name}}</h2>        
    </div>
</div>