<div class="list">
	<div class="item item-checkbox checkbox-balanced item-checkbox-right popup_list" ng-repeat="farmer in farmers">
      <label class="checkbox">
        <input type="checkbox" ng-model="farmer.checked"  
        ng-checked="farmer.checked" ng-change="changeFarmer({{$index}})">   
      </label>
      <h2 class="popup_label">{{farmer.farmerName}}</h2>
      <p class="popup_label_2">{{farmer.farmerCode}}</p>        
    </div>
</div>